input[type=range] {
  position: absolute;
  top: 50%;
  left: 50%;
  border: none;
  padding: 0;
  width: 330px;
  height: 44px;
  border-radius: .25em;
  transform: translate(-50%, -50%);
  font-size: 1em;
  cursor: pointer;
  -webkit-appearance: none;
  &::-webkit-slider-runnable-track, &::-webkit-slider-thumb {
    -webkit-appearance: none;
  }
  &::-webkit-slider-runnable-track {
    width: 330px;
    height: 9px;
    border-radius: 0;
    background: #E2E2E2;
  }
}

.js input[type=range]::-webkit-slider-runnable-track {
  background: linear-gradient(#AB5787 1px, #AB5787, #AB5787) no-repeat #E2E2E2;
  background-size: 10% 100%;
}

input[type=range]::-moz-range-track {
  width: 330px;
  height: 9px;
  border-radius: 0;
  background: #E2E2E2;
}

.js input[type=range]::-moz-range-track {
  background: linear-gradient(#AB5787 1px, #AB5787, #AB5787) no-repeat #E2E2E2;
  background-size: 10% 100%;
}

input[type=range]::-ms-track {
  border: none;
  width: 330px;
  height: 9px;
  border-radius: 0;
  background: #E2E2E2;
  color: transparent;
}
input[type=range] {
  &::-ms-fill-lower {
    border-radius: 4.5px;
    background: linear-gradient(#AB5787 1px, #AB5787, #AB5787);
  }
  &::-webkit-slider-thumb {
    position: relative;
    margin-top: -6.5px;
    border: none;
    width: 22px;
    height: 22px;
    border-radius: 20px;
    background: #54364A;
    background-size: 100% 22px, 100% 100%;
    cursor: ew-resize;
  }
  &::-moz-range-thumb, &::-ms-thumb {
    border: none;
    width: 22px;
    height: 22px;
    border-radius: 20px;
    background: #54364A;
    background-size: 100% 22px, 100% 100%;
    cursor: ew-resize;
  }
  &:focus {
    outline: 0;
    box-shadow: none;
  }
}

